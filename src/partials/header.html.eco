<html lang="en">
	<head>
		<title><%= @getPreparedTitle() %></title>
		<meta http-equiv="content-type" content="text/html; charset=utf-8" />
		<meta name="description" content="" />
		<meta name="keywords" content="" />
		<%- @getBlock("scripts").add(["js/jquery.min.js" ,"js/skel.min.js","js/skel-layers.min.js","js/init.js","js/local.js", @getSiteScripts()]).toHTML() %>
  	<%- @getBlock("styles").add(["css/skel.css","css/style.css","css/style-xlarge.css", "css/local.css", @getSiteStyles()]).toHTML() %>
	</head>
	<body class="<%= @bodyclass %>">
		<!-- Header -->
			<header id="header">
				<nav id="nav">
					<div class="dropdown dropdown-accordion" data-accordion="#accordion">
			      <div class="panel-group" id="accordion">
							<% for page in @getCollection("pages").toJSON(): %>
			        <div class="panel panel-default qcif-menu <%= if page.id is @document.id then 'active' else 'inactive' %>">
			          <div class="panel-heading">
			            <h4 class="panel-title">
			              <a href="<%= @getUrl(page) %>" data-target="#<%= page.title %>" data-toggle="collapse" data-parent="#accordion">
				            	<%= page.title %>
			              </a>
			            </h4>
			          </div>
			          <div class="panel-collapse collapse">
			            <div class="panel-body">
			              <a href="#">Item a</a><br>
			              <a href="#">Item b</a>
			            </div>
			          </div>
			        </div>
							<% end %>
					  </div>
					</div>
				</nav>
			</header>
