<div class="4u 12u$(medium)">
  <section class="box">
    <% if @document.iconclass: %>
    <i class="icon big rounded <%= @document.iconclass %>"></i>
    <% else: %>
      <a href="#" class="image"><img src="images/<%= @document.icon %>" alt="@document.icon" width="150px" height="130px" /></a>
    <% end %>
    <%- @content %>
  </section>
</div>
